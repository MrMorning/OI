class KingdomAndCities {
	public:
		int bino[2501][51];
		int f[51][51], g[51][51];
		int howMany(int N, int M, int K) {
			bino[0][0] = 1;
			for ( int i = 1; i <= 2500; i ++ ) {
				bino[i][0] = 1;
				for ( int j = 1; j <= min(i, 50); j ++ )
					bino[i][j] = plus(bino[i-1][j], bino[i-1][j-1]);
			}
			for ( int i = 1; i <= N; i ++ )
		}
};


